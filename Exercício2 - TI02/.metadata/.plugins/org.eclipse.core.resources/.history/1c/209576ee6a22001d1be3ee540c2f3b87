import java.util.Scanner;

public class Soma {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		double a = sc.nextDouble(), b = sc.nextDouble();
		
		
		sc.close();
	}
	
	public static double soma(double a, double b) {
		double soma = a + b;
		
		return a;
	}

}
